      function drawChart() {

         var fac1 = <%= factor1 %>
         var fac2 = <%= factor2 %>
         var fac3 = <%= factor3 %>
         var fac4 = <%= factor4 %>
         var fac5 = <%= factor5 %>
         var fac6 = <%= factor6 %>
         var fac7 = <%= factor7 %>

