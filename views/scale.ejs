              ['학업 및 진로', fac2],
              ['자기통제', fac3],
              ['IT환경', fac4],
              ['관계에서의 소외감', fac5],
              ['가정환경', fac6],
              ['부모님의 관심 및 제재', fac7]
            ]);

            var options = {
              chart: {
                 title: '   ',
    //            subtitle: 'Sales, Expenses, and Profit: 2014-2017',
              },
              bars: 'vertical'
            };

            var chart = new google.charts.Bar(document.getElementById('barchart_material'));

            chart.draw(data, options);
         }


          
